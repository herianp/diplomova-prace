{
  "name": "quasar-club-system",
  "version": "0.0.1",
  "description": "Club can manage players in many ways.",
  "productName": "Club Manager",
  "author": "petr.herian <peta.herian@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:rules": "firebase emulators:exec --only firestore,auth ./test-rules.sh",
    "emulators:start": "firebase emulators:start --only firestore,auth",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "@vueuse/core": "^14.2.1",
    "chart.js": "^4.5.0",
    "firebase": "^11.4.0",
    "luxon": "^3.6.0",
    "pinia": "^2.0.11",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-chartjs": "^5.3.2",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@quasar/app-vite": "^2.0.0",
    "@types/luxon": "^3.7.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "happy-dom": "^20.5.0",
    "postcss": "^8.4.14",
    "prettier": "^3.3.3",
    "typescript": "^5.9.3",
    "vite-plugin-checker": "^0.8.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "yarn": ">= 1.21.1"
  }
