{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.js", "/*.css", "/*.ico", "/*.png", "/*.jpg", "/*.gif", "/*.svg"]
  },
  "mimeTypes": {
    ".js": "text/javascript",
    ".css": "text/css"
  },
  "globalHeaders": {
    "cache-control": "public, max-age=31536000, immutable"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}